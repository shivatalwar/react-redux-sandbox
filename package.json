{
  "name": "todos-with-undo",
  "version": "0.0.1",
  "private": true,
  "devDependencies": {
    "react-scripts": "^2.1.3"
  },
  "dependencies": {
    "immutable": "^3.8.2",
    "material-ui": "^0.20.2",
    "node-sass": "^4.11.0",
    "node-sass-chokidar": "1.3.4",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "redux-undo": "^0.6.1",
    "reselect": "^4.0.0"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o public/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o public/ --watch --recursive",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "start-js": "react-scripts start",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
